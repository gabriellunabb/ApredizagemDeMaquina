      "source": [
        "import pandas as pd\n",
        "from sklearn.ensemble import RandomForestClassifier\n",
        "\n",
        "data = pd.read_csv('imc.csv')\n",
        "\n",
        "X = data[['IMC']]\n",
        "y = data['Obeso']\n",
        "\n",
        "clf = RandomForestClassifier(random_state=0)\n",
        "clf.fit(X, y)\n",
        "\n",
        "previsao = clf.predict(pd.DataFrame({'IMC': [18, 49, 12, 33, 39]}))\n",
        "\n",
        "print(previsao)\n"
      ]
    }
  ],
